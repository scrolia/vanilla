            "require": "./dist/init.js"
        },
        "./contexts/scrollcore": {
            "types": "./dist/contexts/scrollcore.d.ts",
            "import": "./dist/contexts/scrollcore.mjs",
            "require": "./dist/contexts/scrollcore.js"
        },
        "./functions/props/get": {
            "types": "./dist/functions/props/get.d.ts",
            "import": "./dist/functions/props/get.mjs",
            "require": "./dist/functions/props/get.js"
        },
        "./functions/props/set": {
            "types": "./dist/functions/props/get.d.ts",
            "import": "./dist/functions/props/set.mjs",
            "require": "./dist/functions/props/set.js"
